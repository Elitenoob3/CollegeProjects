<?xml version="1.0" encoding="UTF-8"?>
<bodydef>

	<body name="ball">
		<is_dynamic/>
		<linear_damping>0</linear_damping>
		<angular_damping>0</angular_damping>
		<allow_sleep/>
		<fixture>
			<density>2</density>
			<friction>0.9</friction>
			<restitution>0</restitution>
			<filter_category_bits>1</filter_category_bits>
			<filter_mask_bits>65535</filter_mask_bits>
			<filter_group_index>0</filter_group_index>
			<polygon>3, 5, 4, 11, 12, 11, 13, 5</polygon>
		</fixture>
	</body>

	<metadata>
		<format>1</format>
		<ptm_ratio>32</ptm_ratio>
	</metadata>

</bodydef>

